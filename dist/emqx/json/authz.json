  {
    "desc": "Authorization using a MongoDB replica set.",
    "fields": [
      {
        "desc": "Backend type.",
        "name": "type",
        "type": {
          "kind": "singleton",
          "name": "mongodb"
        }
      },
      {
        "default": {
          "hocon": "true",
          "oneliner": true
        },
        "desc": "Set to <code>true</code> or <code>false</code> to disable this ACL provider",
        "name": "enable",
        "raw_default": true,
        "type": {
          "kind": "primitive",
          "name": "boolean()"
        }
      },
      {
        "desc": "`MongoDB` collection containing the authorization data.",
        "name": "collection",
        "type": {
          "kind": "primitive",
          "name": "atom()"
        }
      },
      {
        "default": {
          "hocon": "{}",
          "oneliner": true
        },
        "desc": "\nConditional expression that defines the filter condition in the query.\nFilter supports the following placeholders:\n - <code>${username}</code>: Will be replaced at runtime with <code>Username</code> used by the client when connecting\n - <code>${clientid}</code>: Will be replaced at runtime with <code>Client ID</code> used by the client when connecting\n",
        "name": "filter",
        "raw_default": {},
        "type": {
          "kind": "primitive",
          "name": "map()"
        }
      },
      {
        "default": {
          "hocon": "rs",
          "oneliner": true
        },
        "desc": "Replica set.",
        "name": "mongo_type",
        "raw_default": "rs",
        "type": {
          "kind": "singleton",
          "name": "rs"
        }
      },
      {
        "desc": "\nA Node list for Cluster to connect to. The nodes should be separated with commas, such as: `Node[,Node].`\nFor each Node should be: The IPv4 or IPv6 address or the hostname to connect to.\nA host entry has the following form: `Host[:Port]`.\nThe MongoDB default port 27017 is used if `[:Port]` is not specified.\n",
        "name": "servers",
        "type": {
          "kind": "primitive",
          "name": "[term()]"
        }
      },
      {
        "default": {
          "hocon": "unsafe",
          "oneliner": true
        },
        "desc": "Write mode.",
        "name": "w_mode",
        "raw_default": "unsafe",
        "type": {
          "kind": "enum",
          "symbols": [
            "unsafe",
            "safe"
          ]
        }
      },
      {
        "default": {
          "hocon": "master",
          "oneliner": true
        },
        "desc": "Read mode.",
        "name": "r_mode",
        "raw_default": "master",
        "type": {
          "kind": "enum",
          "symbols": [
            "master",
            "slave_ok"
          ]
        }
      },
      {
        "desc": "Name of the replica set.",
        "name": "replica_set_name",
        "type": {
          "kind": "primitive",
          "name": "binary()"
        }
      },
      {
        "default": {
          "hocon": "false",
          "oneliner": true
        },
        "desc": "Use DNS SRV record.",
        "name": "srv_record",
        "raw_default": false,
        "type": {
          "kind": "primitive",
          "name": "boolean()"
        }
      },
      {
        "default": {
          "hocon": "8",
          "oneliner": true
        },
        "desc": "Size of the connection pool.",
        "name": "pool_size",
        "raw_default": 8,
        "type": {
          "kind": "primitive",
          "name": "pos_integer()"
        }
      },
      {
        "desc": "EMQX's username in the external database.",
        "name": "username",
        "type": {
          "kind": "primitive",
          "name": "binary()"
        }
      },
      {
        "desc": "EMQX's password in the external database.",
        "name": "password",
        "type": {
          "kind": "primitive",
          "name": "binary()"
        }
      },
      {
        "desc": "Database name associated with the user's credentials.",
        "name": "auth_source",
        "type": {
          "kind": "primitive",
          "name": "binary()"
        }
      },
      {
        "desc": "Database name.",
        "name": "database",
        "type": {
          "kind": "primitive",
          "name": "binary()"
        }
      },
      {
        "name": "topology",
        "type": {
          "kind": "struct",
          "name": "topology"
        }
      },
      {
        "default": {
          "hocon": "{enable = false}",
          "oneliner": true
        },
        "desc": "SSL connection settings.",
        "name": "ssl",
        "raw_default": {
          "enable": false
        },
        "type": {
          "kind": "struct",
          "name": "broker:ssl_client_opts"
        }
      }
    ],
    "full_name": "authz:mongo_rs",
    "paths": [
      "authorization.sources.$INDEX"
    ],
    "tags": [
      "Authorization"
    ]
  },
